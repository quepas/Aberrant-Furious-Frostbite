<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <!-- Set this to your own libs path -->
    <ASSIMP>D:\Programowanie\Biblioteki\Tidy\assimp</ASSIMP>
    <DIRECTX9>D:\Programowanie\Biblioteki\DirectX</DIRECTX9>
    <CATCH>D:\Programowanie\Biblioteki\Tidy\catch</CATCH>
    <!-- End -->
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="ASSIMP">
      <Value>$(ASSIMP)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="DIRECTX9">
      <Value>$(DIRECTX9)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="CATCH">
      <Value>$(CATCH)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
</Project>
